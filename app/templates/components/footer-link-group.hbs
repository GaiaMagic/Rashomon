<div class="footer-group u-paddingLl u-pullLeft">
    <div class="footer-group-name u-marginBl">
        {{grouName}}
    </div>

    <ul>
        {{#each links}}
            <li class="link">
                {{#if url}}
                    <a {{bind-attr href=url}}>
                        {{name}}
                    </a>
                {{else}}
                    {{#link-to route position}}
                        {{name}}
                    {{/link-to}}
                {{/if}}
            </li>
        {{/each}}
    </ul>
</div>
